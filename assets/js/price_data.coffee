
return if @pricedata?

class PriceData
  constructor: (id)->
    @data = DATA_SOURCE[id]

  prices: ->
    $.unique @data.map (d)-> d[1]
      .sort (d1, d2)-> d1 - d2

  names: (price)->
    @data.filter (d)-> d[1] == price
      .map (d)-> d[0]


